                "isTodayValueCreated": true,
                "dateFormat": "mm/dd/yy hh:mm p",
                "timezone": null,
                "isCurrentUser": false,
                "isCurrentUserOwner": true,
                "isNowTimeValue": false,
                "timeFormat": "HH:mm",
                "isMultiple": false,
                "isAjax": false,
                "dataOptions": [],
                "ajaxDataUrl": "",
                "isChecked": false,
                "vif": "",
                "customCode": "<b-button class=\"btn-mwhq\" variant=\"dark\" @click=\"sendEmail(rowData)\">Send Email</b-button>\r\n<b-button class=\"btn-mwhq\" @click=\"print()\" variant=\"dark\">Print</b-button>\r\n <b-button v-if=\"window.fbUser.email==='nat@mywardrobehq.com' || window.fbUser.email==='sebastian@tangle.io' || window.fbUser.email==='pragmatron@gmail.com'|| window.fbUser.email==='dawidhaczela@gmail.com'\" class=\"btn-mwhq\" @click=\"showSubscription()\" variant=\"dark\">{{textSubBtn}} Subscription Table</b-button>",
                "invisible": false,
                "invisibleCondition": "",
                "invisibleInDataGrid": true,
                "invisibleLabel": true,
                "enableCustomValidation": false,
                "customValidationRule": "",
                "customValidationErrorMessage": "",
                "mask": "",
                "lookupOpenInModalDisabled": false,
                "editorMode": "javascript",
                "propName": "sendEmail",
                "gridId": "vendors"
              },
              {
                "type": "custom",
                "name": "control_ef374340-abb5-11ed-8561-4725786059fa",
                "fieldName": "control_ef374340-abb5-11ed-8561-4725786059fa",
                "label": "report",
                "order": 0,
                "defaultValue": "",
                "value": "",
                "className": "col-md-12",
                "readonly": false,
                "readonlyCondition": "",
                "placeholder": "",
                "required": false,
                "requiredCondition": "",
                "isMultiLine": false,
                "isInteger": false,
                "decimalPlace": 0,
                "isTodayValue": false,
                "isTodayValueCreated": true,
                "dateFormat": "mm/dd/yy hh:mm p",
                "timezone": null,
                "isCurrentUser": false,
                "isCurrentUserOwner": true,
                "isNowTimeValue": false,
                "timeFormat": "HH:mm",
                "isMultiple": false,
                "isAjax": false,
                "dataOptions": [],
                "ajaxDataUrl": "",
                "isChecked": false,
                "vif": "",
                "customCode": "    {{widgetKey}}\r\n    <div id=\"print\">\r\n\r\n      <!-- <div class=\"logo\"><img src=\"https://firebasestorage.googleapis.com/v0/b/tangle-157402.appspot.com/o/-NLvDXaAsLMKE7dEZj5z%2Fmwhq_logo%20BLACK.png?alt=media&amp;token=3c704ff9-5bce-4b87-a4d0-9b12829ce983\" alt=\"logo\"></div> -->\r\n      <div class=\"logo\">\r\n        <div style=\"background-image:url(https://firebasestorage.googleapis.com/v0/b/tangle-157402.appspot.com/o/-NLvDXaAsLMKE7dEZj5z%2Fmwhq_logo%20BLACK.png?alt=media&amp;token=3c704ff9-5bce-4b87-a4d0-9b12829ce983)\" class=\"log-picture\"></div>\r\n      </div>\r\n      <div class=\"top-raport\">\r\n        <h2>December Earnings Report</h2>\r\n        <div>\r\n          <div>\r\n            <p>Vendor Name:</p>\r\n            <p>Payment Date:\r\n            </p>\r\n          </div>\r\n          <div>\r\n            <p>{{vendorName(rowData.email)}}</p>\r\n            <p>{{paymentDate('December')}}\r\n            </p>\r\n          </div>\r\n          <div>\r\n            <p>% Earning from Rental:</p>\r\n            <p>% Earning from Sales:</p>\r\n          </div>\r\n          <div>\r\n            <p>{{homeReportPercentage(rowData.email).perRent}}%</p>\r\n            <p>{{homeReportPercentage(rowData.email).perSale}}%\r\n            </p>\r\n          </div>\r\n          <div>\r\n            <p>Invoice Number</p>\r\n            <p>Payment Period:\r\n            </p>\r\n          </div>\r\n          <div>\r\n            <p>1</p>\r\n            <p>1-31 Dec 2022\r\n            </p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <h2>Monthly Rentals</h2>\r\n      <table class=\"tab-class\">\r\n        <thead>\r\n          <tr>\r\n            <th>Transaction Date</th>\r\n            <th>Transaction ID</th>\r\n            <th>Item Name</th>\r\n            <th>Item SKU</th>\r\n            <th>Sec SKU</th>\r\n            <th>Rental Period</th>\r\n            <th>Gross Rental Price</th>\r\n            <th>Net Rental Price</th>\r\n            <th>MWHQ Commission Rate</th>\r\n            <th>MWHQ Commission</th>\r\n            <th>Vat on MWHQ Commission</th>\r\n            <th>Total MWHQ Deductions from Gross Rental Price</th>\r\n            <th>Net Balance Due to Vendor</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"item in monthlyRentals(rowData.email).rental\">\r\n            <td>{{item.$transactionDate$display}}</td>\r\n            <td>{{item.transactionID }}</td>\r\n            <td class=\"th-name\">{{item.itemName}}</td>\r\n            <td>{{item.itemSKU}}</td>\r\n            <td></td>\r\n            <td>{{item.rentalPeriod}}</td>\r\n            <td>£{{item.mWRent15.toFixed(2)}}</td>\r\n            <td>£{{item.netRentalPrice.toFixed(2)}}</td>\r\n            <td>{{(item.mWHQCommissionRate*100).toFixed(2) }}%</td>\r\n            <td>£{{item.mWHQCommission.toFixed(2) }}</td>\r\n            <td>£{{item.vATonMWHQCommission.toFixed(2)}}</td>\r\n            <td>£{{item.totalMWHQDeductionsfromGrossSalesPrice.toFixed(2)}}</td>\r\n            <td>£{{item.netBalanceDueToVendor.toFixed(2)}}</td>\r\n          </tr>\r\n          <tr class=\"bottom-row\">\r\n            <th colspan=\"3\">Total</th>\r\n            <td>{{rentalTotal(rowData.email).total.totalItem}}</td>\r\n            <td colspan=\"3\"></td>\r\n            <td>£{{rentalTotal(rowData.email).total.totalNet.toFixed(2)}}</td>\r\n            <td>{{rentalTotal(rowData.email).total.totalRate}}%</td>\r\n            <td>£{{rentalTotal(rowData.email).total.totalMWHQcom}}</td>\r\n            <td>£{{rentalTotal(rowData.email).total.totalVat}}</td>\r\n            <td>£{{rentalTotal(rowData.email).total.totalGross}}</td>\r\n            <td>£{{rentalTotal(rowData.email).total.totalBalance}}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <h2 v-if=\"$getUser('showSub')\">Subscription</h2>\r\n      <table class=\"tab-class\" v-if=\"$getUser('showSub')\">\r\n        <thead>\r\n          <tr>\r\n            <th>Transaction Date</th>\r\n            <th>Transaction ID</th>\r\n            <th>Item Name</th>\r\n            <th>Item SKU</th>\r\n            <th>Sec SKU</th>\r\n            <th>Rental Period</th>\r\n            <th>Gross Rental Price</th>\r\n            <th>Net Rental Price</th>\r\n            <th>MWHQ Commission Rate</th>\r\n            <th>MWHQ Commission</th>\r\n            <th>Vat on MWHQ Commission</th>\r\n            <th>Total MWHQ Deductions from Gross Rental Price</th>\r\n            <th>Net Balance Due to Vendor</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr>\r\n            <td></td>\r\n            <td></td>\r\n            <td class=\"th-name\"></td>\r\n            <td></td>\r\n            <td></td>\r\n            <td></td>\r\n            <td>£</td>\r\n            <td>£</td>\r\n            <td>%</td>\r\n            <td>£</td>\r\n            <td>£0.00</td>\r\n            <td>£</td>\r\n            <td>£</td>\r\n          </tr>\r\n          <tr class=\"bottom-row\">\r\n            <th colspan=\"3\">Total</th>\r\n            <td></td>\r\n            <td colspan=\"3\"></td>\r\n            <td>£</td>\r\n            <td>%</td>\r\n            <td>£</td>\r\n            <td>£0.00</td>\r\n            <td>£</td>\r\n            <td>£</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <h2>Monthly Sales</h2>\r\n      <table class=\"tab-class\">\r\n        <thead>\r\n          <tr>\r\n            <th>Transaction Date</th>\r\n            <th>Transaction ID</th>\r\n            <th>Item Name</th>\r\n            <th>Item SKU</th>\r\n            <th>Sec SKU</th>\r\n            <th>Gross Sale Price</th>\r\n            <th>Net Sales Price</th>\r\n            <th>MWHQ Commission Rate</th>\r\n            <th>MWHQ Commission</th>\r\n            <th>Vat on MWHQ Commission</th>\r\n            <th>Total MWHQ Deductions from Gross Sales Price</th>\r\n            <th>Net Balance Due to Vendor</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"item in monthlySales(rowData.email).sales\">\r\n            <td>{{item.$transactionDate$display}}</td>\r\n            <td>{{item.transactionID }}</td>\r\n            <td class=\"th-name\">{{item.itemName}}</td>\r\n            <td>{{item.itemSKU}}</td>\r\n            <td></td>\r\n            <td>£{{item.grossSalesPrice.toFixed(2) }}</td>\r\n            <td>£{{item.netSalesPrice.toFixed(2)}}</td>\r\n            <td>{{(item.mWHQCommissionRate*100).toFixed(2) }}%</td>\r\n            <td>£{{item.mWHQCommission.toFixed(2) }}</td>\r\n            <td>£{{item.vATonMWHQCommission.toFixed(2)}}</td>\r\n            <td>£{{item.totalMWHQDeductionsfromGrossSalesPrice.toFixed(2)}}</td>\r\n            <td>£{{item.netBalanceDueToVendor.toFixed(2)}}</td>\r\n          </tr>\r\n          <tr class=\"bottom-row\">\r\n            <th colspan=\"3\">Total</th>\r\n            <td>{{salesTotal(rowData.email).total.totalItem}}</td>\r\n            <td colspan=\"2\"></td>\r\n            <td>£{{salesTotal(rowData.email).total.totalNet}}</td>\r\n            <td>{{salesTotal(rowData.email).total.totalRate}}%</td>\r\n            <td>£{{salesTotal(rowData.email).total.totalMWHQcom}}</td>\r\n            <td>£{{salesTotal(rowData.email).total.totalVat}}</td>\r\n            <td>£{{salesTotal(rowData.email).total.totalGross}}</td>\r\n            <td>£{{salesTotal(rowData.email).total.totalBalance}}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <div class=\"box-small-table\">\r\n        <h3>INVOICE SUMMARY</h3>\r\n        <table class=\"small-tab\">\r\n          <thead>\r\n            <tr>\r\n              <th></th>\r\n              <th>Due to MWHQ</th>\r\n              <th>Due To Vendor</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr>\r\n              <th>Rental Commissions</th>\r\n              <td>£{{rentalTotal(rowData.email).total.totalMWHQcom}}</td>\r\n              <td>£{{rentalTotal(rowData.email).total.totalBalance}}</td>\r\n            </tr>\r\n            <tr>\r\n              <th>Sale Commissions</th>\r\n              <td>£{{salesTotal(rowData.email).total.totalMWHQcom}}</td>\r\n              <td>£{{salesTotal(rowData.email).total.totalBalance}}</td>\r\n            </tr>\r\n            <!-- <tr>\r\n <th>Return Commissions</th> <td>£0.00</td> <td>£0.00</td>\r\n </tr> -->\r\n            <tr class=\"bottom-row\">\r\n              <th>Sub-Total</th>\r\n              <td>£{{(parseFloat(rentalTotal(rowData.email).total.totalMWHQcom) + parseFloat(salesTotal(rowData.email).total.totalMWHQcom)).toFixed(2)}}</td>\r\n              <td>£{{(parseFloat(rentalTotal(rowData.email).total.totalBalance) + parseFloat(salesTotal(rowData.email).total.totalBalance)).toFixed(2) }}</td>\r\n            </tr>\r\n            <tr>\r\n              <th>MWHQ VAT</th>\r\n              <td>£{{(parseFloat(rentalTotal(rowData.email).total.totalVat) + parseFloat(salesTotal(rowData.email).total.totalVat)).toFixed(2)}}</td>\r\n              <td></td>\r\n            </tr>\r\n            <tr class=\"bottom-row\">\r\n              <th>Total Due To MWHQ</th>\r\n              <td>£{{(parseFloat(rentalTotal(rowData.email).total.totalMWHQcom) + parseFloat(salesTotal(rowData.email).total.totalMWHQcom) + parseFloat(rentalTotal(rowData.email).total.totalVat) + parseFloat(salesTotal(rowData.email).total.totalVat)).toFixed(2) }}</td>\r\n              <td></td>\r\n            </tr>\r\n            <tr>\r\n              <th>Total Due To Vendor</th>\r\n              <td></td>\r\n              <td class=\"vendor-total\">£{{(parseFloat(rentalTotal(rowData.email).total.totalBalance) + parseFloat(salesTotal(rowData.email).total.totalBalance)).toFixed(2) }}</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n    <style>\r\n      td,\r\n      tr,\r\n      th {\r\n        border: 1px solid black;\r\n      }\r\n\r\n      th {\r\n        padding: .3em;\r\n        text-align: center;\r\n        background-color: #f8f9fe;\r\n      }\r\n\r\n      td {\r\n        text-align: right;\r\n        padding-right: .3em;\r\n\r\n      }\r\n\r\n      .th-name {\r\n        text-align: center;\r\n\r\n      }\r\n\r\n      .bottom-row {\r\n        border-top: 3px solid black;\r\n      }\r\n\r\n      h2 {\r\n        padding: .8em;\r\n        width: 100%;\r\n        text-align: center;\r\n        font-weight: bold;\r\n      }\r\n\r\n      .tab-class {\r\n        margin: auto;\r\n\r\n      }\r\n\r\n      .top-raport {\r\n\r\n        margin: auto;\r\n        background-color: #f8f9fe;\r\n      }\r\n\r\n      .top-raport>div {\r\n        width: 100%;\r\n        display: flex;\r\n        justify-content: space-between;\r\n\r\n      }\r\n\r\n      .top-raport>div div {\r\n        display: flex;\r\n        flex-direction: column;\r\n        width: 100%;\r\n        text-align: right;\r\n        padding: .5em;\r\n      }\r\n\r\n      .sub-title {\r\n        width: 100%;\r\n        text-align: center;\r\n        color: rgba(0, 0, 0, .2);\r\n        letter-spacing: .1rem;\r\n        padding: .1em;\r\n      }\r\n\r\n      h1 {\r\n        width: 100%;\r\n        text-align: center;\r\n        letter-spacing: .3rem;\r\n      }\r\n\r\n      .small-tab {\r\n        width: 50%;\r\n        margin-top: .3em;\r\n\r\n      }\r\n\r\n      .vendor-total {\r\n        background-color: #bec7f7;\r\n      }\r\n\r\n      .box-small-table {\r\n\r\n\r\n        display: flex;\r\n        flex-direction: column;\r\n        margin: auto;\r\n      }\r\n\r\n      h3 {\r\n        margin-top: 1em;\r\n        padding: .5em;\r\n        font-weight: bold;\r\n      }\r\n\r\n      .select-box {\r\n        width: 50%;\r\n        display: flex;\r\n        justify-content: end;\r\n      }\r\n\r\n      .box-header {\r\n        display: flex;\r\n        width: 100%;\r\n        justify-content: space-around;\r\n      }\r\n\r\n      h1,\r\n      h5,\r\n      .top-raport,\r\n      h2,\r\n      table,\r\n      h3,\r\n      .btn-mwhq {\r\n        font-family: 'Futura' !important;\r\n      }\r\n\r\n      .btn-mwhq {\r\n        text-transform: uppercase;\r\n        letter-spacing: .1rem;\r\n      }\r\n\r\n      .logo {\r\n        display: flex;\r\n        justify-content: center;\r\n        margin: auto;\r\n\r\n      }\r\n\r\n      .logo>img {\r\n        width: 300px;\r\n        height: 100px;\r\n        object-fit: contain\r\n      }\r\n\r\n      .log-picture {\r\n        background-image: url(\"https://firebasestorage.googleapis.com/v0/b/tangle-157402.appspot.com/o/-NLvDXaAsLMKE7dEZj5z%2Fmwhq_logo%20BLACK.png?alt=media&amp;token=3c704ff9-5bce-4b87-a4d0-9b12829ce983\");\r\n        background-size: contain;\r\n        background-position: center;\r\n        background-repeat: no-repeat;\r\n        width: 400px;\r\n        height: 100px;\r\n        margin: auto;\r\n      }\r\n\r\n      @media print {\r\n\r\n        h1,\r\n        h5,\r\n        .top-raport,\r\n        h2,\r\n        table,\r\n        h3,\r\n        .btn-mwhq {\r\n          font-family: 'Futura' !important;\r\n        }\r\n      }\r\n    </style>\r\n  </div>\r\n</div>",
